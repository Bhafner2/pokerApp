    apiKey: <your firrbase config>,
    authDomain: <your firrbase config>,
    databaseURL: <your firrbase config>,
    projectId: <your firrbase config>,
    storageBucket: <your firrbase config>,
    messagingSenderId: <your firrbase config>,
